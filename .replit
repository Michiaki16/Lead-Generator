modules = ["nodejs-20", "python-3.11"]
[agent]
expertMode = true
integrations = ["web_scraper==1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["alsa-lib", "atk", "cairo", "cups", "dbus", "gdk-pixbuf", "glib", "gtk3", "libdrm", "mesa", "nss", "xorg.libX11", "xorg.libXcomposite", "xorg.libXdamage", "xorg.libXext", "xorg.libXfixes", "xorg.libXrandr"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Lead Generator App"

[[workflows.workflow]]
name = "Lead Generator App"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm install && npm start"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"
